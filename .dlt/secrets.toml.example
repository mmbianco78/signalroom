# dlt Secrets Configuration (TEMPLATE)
# Copy to secrets.toml and fill in your credentials.
# NEVER commit secrets.toml to git!
#
# See: https://dlthub.com/docs/general-usage/credentials/configuration

# =============================================================================
# Source Credentials
# =============================================================================

[sources.everflow]
api_key = "your-everflow-api-key"

[sources.redtrack]
api_key = "your-redtrack-api-key"

[sources.posthog]
api_key = "your-posthog-api-key"

# =============================================================================
# Destination Credentials
# =============================================================================

[destination.postgres]
# Supabase connection string
credentials = "postgresql://postgres.PROJECT_ID:PASSWORD@aws-0-REGION.pooler.supabase.com:6543/postgres"

# =============================================================================
# Notes
# =============================================================================
#
# 1. Copy this file to secrets.toml (remove .example)
# 2. Fill in real credentials
# 3. Ensure secrets.toml is in .gitignore
# 4. For production, use environment variables instead:
#    - SOURCES__EVERFLOW__API_KEY
#    - DESTINATION__POSTGRES__CREDENTIALS
